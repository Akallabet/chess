{
  "name": "@chess/chess",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node --test --loader tsx test/**/*.test.ts",
    "test:watch": "node --test --watch --loader tsx test/**/*.test.ts",
    "emit:types": "tsc -p . --emitDeclarationOnly",
    "bundle": "swc src -d dist -q",
    "performances": "node ./scripts/performances.js",
    "prebuild": "rm -rf dist",
    "build": "npm run bundle",
    "postbuild": "npm run emit:types"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tsx": "^3.12.6",
    "typescript": "^4.9.4"
  }
}
